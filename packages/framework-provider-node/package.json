{
  "name": "@boostercloud/framework-provider-node",
  "version": "0.20.2",
  "description": "Run Booster with NodeJS and MongoDB",
  "keywords": [
    "framework-provider-node"
  ],
  "author": "advantitge",
  "homepage": "https://advantitge.com",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boostercloud/booster.git"
  },
  "scripts": {
    "format": "prettier --write --ext '.js,.ts' **/*.ts **/*/*.ts",
    "lint:check": "eslint --ext '.js,.ts' **/*.ts",
    "lint:fix": "eslint --quiet --fix --ext '.js,.ts' **/*.ts",
    "compile": "tsc -b tsconfig.json",
    "clean": "rimraf ./dist tsconfig.tsbuildinfo",
    "prepack": "tsc -b tsconfig.json",
    "test:provider-local": "npm run test",
    "test": "BOOSTER_ENV=test nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\""
  },
  "bugs": {
    "url": "https://github.com/boostercloud/booster/issues"
  },
  "dependencies": {
    "@boostercloud/framework-common-helpers": "^0.20.2",
    "@boostercloud/framework-core": "^0.20.2",
    "@boostercloud/framework-types": "^0.20.2",
    "micro": "9.3.4",
    "micro-cors": "0.1.1",
    "mongodb": "4.0.1",
    "tslib": "2.3.0"
  },
  "devDependencies": {
    "@types/faker": "5.1.5",
    "@types/micro": "7.3.4",
    "@types/micro-cors": "0.1.2",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "faker": "5.1.0",
    "sinon": "9.2.3",
    "sinon-chai": "3.5.0"
  }
}
